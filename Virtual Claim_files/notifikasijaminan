$(document).ready(function(){function t(){$.ajax({type:"GET",url:$("#txturl").val()+"/Laporan/lapJaminanNotifikasiStatusKirim",data:"hari="+LZString.compressToEncodedURIComponent(JSON.stringify("10")),success:function(t){var u=JSON.parse(LZString.decompressFromEncodedURIComponent(t)),e,f,r,i;if(u.pesan.kode=="1"){for(e=0,f=0,i=0;i<u.jaminan.length;i++)f=f+1;for(r='<a href="#" class="dropdown-toggle" data-toggle="dropdown"> <i class="fa fa-envelope-o"><\/i> <span class="label label-danger">'+f+"<\/span> <\/a>",r+='<ul class="dropdown-menu">',r+='<li class="header">Data Gagal Kirim (5 Data Terakhir)<\/li>',r+='<li> <ul class="menu">',i=0;i<u.jaminan.length;i++)if(i<5)r+='<li> <a href="#"> <div class="pull-left"> <img src="../image/bpjsIcon.ico" class="img-circle" alt="User Image"> <\/div> <h4> '+u.jaminan[i].nosep+"<\/h4> <p> Tgl.SEP : "+u.jaminan[i].tglsep+"<\/p> <p> Keterangan : "+u.jaminan[i].returnjr+"<\/p> <\/a> <\/li>";else break;r+="<\/ul><\/li>";r+='<li class="footer"><a href='+n+">See All Data ("+f+") <\/a><\/li>";r+="<\/ul>";$("#li_notifikasi_JR").html(r)}}})}t();var n=$("#txturl").val()+"/Jaminan/JaminanJRGagalKirim"})