$(document).ready(function(){function t(t){$.ajax({url:$("#txturl").val()+"/Jaminan/getLaporanPolisi",data:"lp="+LZString.compressToEncodedURIComponent(JSON.stringify(t)),type:"GET",dataType:"json",success:function(t){var r=JSON.parse(LZString.decompressFromEncodedURIComponent(t));r.pesan.kode=="1"?(i(r.irsms),$("#divBody").show(),$("#divfooter").show()):(notif("Informasi..!!",r.pesan.pesan,"info"),$("#divBody").hide(),n())}})}function i(n){$("#lblnolp").html($("#txtcarinolp").val());$("#lbltglkejadian").html(n.tanggal_kejadian);$("#lbltgldilaporkan").html(n.tanggal_dilaporkan);$("#lblpolres").html(n.polda+" / "+n.polres);$("#lbltmpkerjadian").html(n.tempat_terjadinya_kecelakaan_lalu_lintas);$("#lbltipekecelakaan").html(n.tipe_kecelakaan);$("#lbltingkatkecelakaan").html(n.tingkat_kecelakaan);$("#lblmeninggal").html("Meninggal Dunia : "+n.jumlah_terluka.meninggal_dunia+" Orang");$("#lblberat").html("Luka Berat : "+n.jumlah_terluka.luka_berat+" Orang");$("#lblringan").html("Luka Ringan : "+n.jumlah_terluka.luka_ringan+" Orang")}function n(){$("#txtcarinolp").val("");$("#lblnolp").html("");$("#lbltglkejadian").html("");$("#lbltgldilaporkan").html("");$("#lblpolres").html("");$("#lbltmpkerjadian").html("");$("#lbltipekecelakaan").html("");$("#lbltingkatkecelakaan").html("");$("#lblmeninggal").html("");$("#lblberat").html("")}$("#txturl").val(base_url);$("#divBody").hide();$("#divfooter").hide();n();$("#btnclose").click(function(){$("#divBody").hide();$("#divfooter").hide();n()});$("#btnCariLP").click(function(){$("#divBody").hide();$("#divfooter").hide();t($("#txtcarinolp").val())});$("#btnCetakLP").click(function(){var t=$("#txturlcetak").val()+$("#txtcarinolp").val(),n=window.open("","_blank","directories=no,titlebar=nostatus=no,toolbar=no,menubar=no,navigationbar=no,location=no,resizable=no,scrollbars=no,width=1024,height=600");n.location.href=="about:blank"&&(n.location.href=t)})})